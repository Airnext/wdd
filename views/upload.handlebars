  <main id="main" class="main-page">

    <!--==========================
      Upload CV Section
    ============================-->
    <section id="upload" class="wow fadeIn">
      <div class="container">
        <div class="upload-area">
          <form method="post" action="/uploader" enctype="multipart/form-data">
            <fieldset>
              <legend>Upload CV</legend>
              {{#if uploadSuccess}}
                <p class="alert alert-success text-center" id="success">{{uploadSuccess}} <span class="close" data-dismiss="alert">&times;</span></p>
              {{/if}}
              <p class="alert alert-danger text-center" id="display"> <span class="close" data-dismiss="alert">&times;</span></p>
              <div class="form-group row">
                <label for="email" class="col-sm-2 col-md-2 col-lg-2 col-form-label">Email</label>
                <div class="col-sm-10 col-md-10 col-lg-10">
                  <input type="email" class="form-control" id="email" name="email" placeholder="Enter Email" aria-describedby="emailHelpBlock" id="upload-email" required>
                  <small id="emailHelpBlock" class="form-text text-muted">
                    Ensure that you enter the same email you used in registering and payment.
                  </small>
                </div>
              </div>
              <div class="form-group row">
                <label for="file" class="col-sm-2 col-md-2 col-lg-2 col-form-label">Browse for CV</label>
                <div class="col-sm-10 col-md-10 col-lg-10">
                  <input type="file" class="form-control" id="file" name="file" required disabled="disabled">
                </div>
              </div>  
              <div class="form-group">
                <div class="col-sm-12 text-center">
                  <button type="submit" class="btn btn-success" id="confirm-btn"> Confirm Email</button>
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-12 text-center">
                  <button type="submit" class="btn btn-primary" id="upload-btn" disabled="disabled"><i class="fa fa-cloud-upload"></i> Upload CV</button>
                </div>
              </div>
              </fieldset>
          </form>     
        </div>
      </div>
    </section>

  </main>


